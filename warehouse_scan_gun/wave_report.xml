<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_list_wave_report" model="ir.ui.view">
            <field name="name">view_list_wave_repot</field>
            <field name="model">wave.report</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <tree string="Wave Reports ">
                    <field name="id"/>
                    <field name="pack_id"/>
                    <field name="product_id"/>
                    <field name="lot_id"/>
                    <field name="product_qty"/>
                    <field name="uos_qty"/>
                    <field name="uom_qty_ok"/>
                    <field name="uos_qty_ok"/>
                 </tree>
            </field>
        </record>

        <record id="action_list_wave_report" model="ir.actions.act_window">
            <field name="name">Wave Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wave.report</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
        </record>


    </data>
</openerp>
